WORKDIR = `pwd`

CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = -Wall -fPIC -m64 -fexceptions
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = 

INC_RELEASE = $(INC) -Iinclude
CFLAGS_RELEASE = $(CFLAGS) -O2
RESINC_RELEASE = $(RESINC)
RCFLAGS_RELEASE = $(RCFLAGS)
LIBDIR_RELEASE = $(LIBDIR) -Llib/linux_x64
LIB_RELEASE = $(LIB)-lssl -lcrypto
LDFLAGS_RELEASE = $(LDFLAGS) -s
OBJDIR_RELEASE = obj/Release
DEP_RELEASE = 
OUT_RELEASE = ./linux/Release64/libanychatsign.so

OBJ_RELEASE = $(OBJDIR_RELEASE)/jnihelper.o $(OBJDIR_RELEASE)/libanychatsign.o

all: release

clean: clean_release

before_release: 
	test -d ./linux/Release64 || mkdir -p ./linux/Release64
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(LD) -shared $(LIBDIR_RELEASE) $(OBJ_RELEASE)  -o $(OUT_RELEASE) $(LDFLAGS_RELEASE) $(LIB_RELEASE)

$(OBJDIR_RELEASE)/jnihelper.o: jnihelper.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c jnihelper.cpp -o $(OBJDIR_RELEASE)/jnihelper.o

$(OBJDIR_RELEASE)/libanychatsign.o: libanychatsign.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c libanychatsign.cpp -o $(OBJDIR_RELEASE)/libanychatsign.o

clean_release: 
	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf ../../linux/Release
	rm -rf $(OBJDIR_RELEASE)

.PHONY: before_release after_release clean_release

